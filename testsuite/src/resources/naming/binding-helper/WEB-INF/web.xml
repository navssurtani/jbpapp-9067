<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">    
    
    <description>Helper war to help create desired JNDI structure</description>
    
    <servlet>
        <servlet-name>NamingUtilServlet</servlet-name>
        <servlet-class>org.jboss.test.naming.util.NamingUtilServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NamingUtilServlet</servlet-name>
        <url-pattern>/naming-util-servlet</url-pattern>
    </servlet-mapping>   
</web-app>